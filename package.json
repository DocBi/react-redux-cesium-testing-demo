{
  "name": "react-redux-cesium-testing-demo",
  "version": "1.0.0",
  "description": "A sample project to demonstrate usage of React, Redux, Cesium, Webpack, and Mocha.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node devServer.js",
    "build": "rimraf \"dist/*\" && cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.config.js && node ./webpack/copyCesium.js",
    "start": "node run dev",
    "test": "npm run test:build && mocha --require test/mocha_helper.js disttest/appTestBundle.js",
    "lint": "eslint src --ext .js --ext .jsx || exit 0",
    "test:build": "rimraf \"distTest/*\" && cross-env NODE_ENV=test webpack --config ./webpack/webpack.test.config.js --target node",
    "test:run": "mocha --require test/mocha_helper.js disttest/appTestBundle.js || exit 0",
    "test:dev": "cross-env NODE_ENV=test node testServer.js"
  },
  "author": "Mark Erikson",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-class-display-name": "^0.1.0",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-resolver": "0.0.18",
    "cesium": "^1.18.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "chai-enzyme": "^0.4.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.0.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^3.16.1",
    "exports-loader": "^0.6.3",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.26.5",
    "glob": "^7.0.0",
    "html-webpack-plugin": "^2.8.1",
